<%= form_for :pessoa, url: pessoas_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :name %>
  </p>
  
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :last_name %>
  </p>
  
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :email %>
  </p>
  
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :company %>
  </p>
  
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :job_title %>
  </p>
  
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :phone %>
  </p>
  
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :website %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>